function traverse2dArray(N, M, matrix){
    let bag="";
    for(let row=M-1; row>=0; row--){
       
        for(let col=0; col<N; col++){
            bag=bag+matrix[col][row]+" ";
        }
    }console.log(bag);
    
   }
   
    